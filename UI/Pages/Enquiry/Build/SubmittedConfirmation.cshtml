@page
@model UI.Pages.Enquiry.Build.SubmittedConfirmation
@using GovUk.Frontend.AspNetCore.TagHelpers
@{
    ViewData["Title"] = "Enquiry submitted confirmation";
    ViewData["IncludePrintPage"] = true;
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <govuk-notification-banner type="Success">
            <p class="govuk-notification-banner__heading">
                Request sent
            </p>
        </govuk-notification-banner>
        <h1 class="govuk-heading-l">What happens next</h1>
        <p class="govuk-body">
            You'll be notified in 10 working days if tuition partners can work with you.
        </p>
        <p class="govuk-body">
            If you don't get any responses from tuition partners, you'll be emailed in 10 working days with more information about what to do next
        </p>
    </div>
</div>