@model Application.Common.Models.ShortlistCheckboxModel
<govuk-checkboxes class="govuk-checkboxes--small govuk-!-padding-bottom-0 govuk-!-margin-1" name="@Model.CheckboxName" data-module="shortlist-checkbox">
    <govuk-checkboxes-item id="@Model.Id" value="@Model.SeoUrl" checked="@Model.IsShortlisted">
            <span class="shortlist-partner-checkbox-label--font-style">Add @Model.LabelValue to my shortlist</span>
        </govuk-checkboxes-item>
</govuk-checkboxes>