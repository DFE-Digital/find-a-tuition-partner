@page
@model UI.Pages.Session.Clear
@{
    ViewData["Title"] = "Session timeout";
    ViewData["IncludeHomeLink"] = true;
}

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <h1 class="govuk-heading-l">
            Your answers have been deleted
        </h1>
        <p class="govuk-body">
            If you still want to use this service, you’ll need to start again.
        </p>
        <a class="govuk-button" data-module="govuk-button" draggable="false" href="/" role="button">Start again</a>
    </div>
</div>
